<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<DefineConstants>NET9_0</DefineConstants>
		<RazorCompileOnBuild>false</RazorCompileOnBuild>
		<RazorCompileOnPublish>false</RazorCompileOnPublish>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>
		<RuntimeIdentifiers>win-x64</RuntimeIdentifiers>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>TRACE;RELEASE;STAGING;$(SyncfusionLicensing)</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>TRACE;DEBUG;STAGING;$(SyncfusionLicensing)</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(STAGING)' != ''">
		<DefineConstants>$(DefineConstants);STAGING</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Condition="'$(REDIS)' != ''">
		<DefineConstants>$(DefineConstants);REDIS</DefineConstants>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="Models\**" />
	  <Compile Include="Models\Animation.cs" />
	  <Compile Include="Models\EmployeeShiftManagementData.cs" />
	  <Compile Include="Models\EventManagementData.cs" />
	  <Compile Include="Models\FifaStatisticsData.cs" />
	  <Compile Include="Models\PivotTableData.cs" />
	  <Compile Include="Models\PromptResponseData.cs" />
	  <Compile Include="Models\SalesData.cs" />
	  <Compile Include="Models\TelecastData.cs" />
	  <Compile Include="Models\ScheduleData.cs" />
	  <Compile Include="Models\ScheduleEventsData.cs" />
	  <Compile Include="Models\VegetableData.cs" />
	  <Compile Include="Models\ChatMessagesData.cs" />
	  <Compile Include="Models\BlockData.cs" />
	  <Content Remove="Models\**" />
	  <EmbeddedResource Remove="Models\**" />
	  <None Remove="Models\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" Version="9.0.4" />
		<PackageReference Include="Microsoft.Extensions.Caching.Redis" Version="2.3.0" Condition="$(DefineConstants.Contains('REDIS'))" />
		<PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="9.0.4" />
		<PackageReference Include="Syncfusion.EJ2.AspNet.Core" Version="31.2.2" />
		<PackageReference Include="Syncfusion.EJ2.GridExport.Core" Version="31.2.2" />
		<PackageReference Include="Syncfusion.Compression.Net.Core" Version="31.2.2" />
		<PackageReference Include="Syncfusion.Licensing" Version="31.2.2" />
		<PackageReference Include="System.Net.Http" Version="4.3.4" />
		<PackageReference Include="System.Text.RegularExpressions" Version="4.3.1" />
		<PackageReference Include="System.Text.Json" Version="9.0.4" />
	</ItemGroup>

	<ItemGroup>
		<Compile Remove="Startup.cs" />
		<Compile Remove="wwwroot\**\*;node_modules;runtimeconfig.template.json;" />
		<None Update="wwwroot\**\*">
			<CopyToPublishDirectory>Always</CopyToPublishDirectory>
		</None>
		<Content Include="Pages\**\*.html">
			<CopyToPublishDirectory>Always</CopyToPublishDirectory>
		</Content>
		<None Include="Pages\**\*.cs" CopyToOutputDirectory="Always" Condition="'$(Configuration)'=='Release'" />
		<None Include="Models\**\*.cs" CopyToOutputDirectory="Always" Condition="'$(Configuration)'=='Release'" />
		<None Include="Controllers\**" CopyToOutputDirectory="Always" Condition="'$(Configuration)'=='Release'" />
	</ItemGroup>

