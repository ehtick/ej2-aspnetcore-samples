            </e-chart-tooltipsettings>
        </ejs-chart>
    </div>
    <script src="~/scripts/chart/financial-data.js"></script>
    <script src="~/scripts/chart/theme-color.js"></script>
    <script type="text/javascript">
        var ChartPoints = annotationData;
        function load(args) {
            var selectedTheme = loadChartTheme(args);
            args.chart.series[0].dataSource = ChartPoints ;
            args.chart.annotations[7].content = ej.base.Browser.isDevice ? '' : '<div class="third-box-bottom" >Junção</div>';
            args.chart.annotations[8].content = ej.base.Browser.isDevice ? '' : '<div class="box-top">Subida dos <br/> Boxes</div>';
            args.chart.annotations[9].content = ej.base.Browser.isDevice ? '' : '<div style="font-family: sans-serif">Max, accelertion <br/> 5.00 g at 5th gear</div>';
            args.chart.annotations[10].content = ej.base.Browser.isDevice ? '' : '<div style="font-family: sans-serif">Max, accelertion <br/> 4.58 g at 5th gear</div>';
            if (selectedTheme.match('Dark')) {
                args.chart.series[0].fill = 'url(#dark-gradient-chart)';
            }
            else {
                args.chart.series[0].fill = 'url(#gradient-chart)';
            }
            args.chart.width = ej.base.Browser.isDevice ? "100%" : "90%";
        };

        function axisLabelRender(args) {
            if (args.axis.name === 'primaryXAxis') { args.text = args.text + ' KM'; }
        };
    </script>
}
