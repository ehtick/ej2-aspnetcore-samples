                    <e-series-marker width="7" height="7" fill="rgb(247, 206, 105,0.7)"></e-series-marker>
                </e-series>
            </e-series-collection>
            <e-chart-annotations>
                <e-chart-annotation coordinateUnits="@Syncfusion.EJ2.Charts.Units.Point" x="0.360" y="80" content="<div class='first-box-bottom'>Senna S</div>">
