@page
@model EJ2CoreSampleBrowser.Pages.Chart.DataLabelTemplateModel
@using Syncfusion.EJ2

@{
    var margin = new { Right = 30 };
    var margin1 = new { Right = 15 };
}

@section ControlsSection{
    <div class="control-section" align="center">
        <ejs-chart id="container" textRender="textRender" load="load" title="Athletes in Popular School">
            <e-chart-chartarea>
                <e-chartarea-border width="0"></e-chartarea-border>
            </e-chart-chartarea>
            <e-chart-subtitlestyle textAlignment="@Syncfusion.EJ2.Charts.Alignment.Far">
            </e-chart-subtitlestyle>
            <e-chart-legendsettings enableHighlight="false">
            </e-chart-legendsettings>
            <e-chart-primaryxaxis valueType="@Syncfusion.EJ2.Charts.ValueType.Category"
                                  edgeLabelPlacement="@Syncfusion.EJ2.Charts.EdgeLabelPlacement.Shift">
                <e-majorgridlines width="0"></e-majorgridlines>
                <e-majorticklines width="0"></e-majorticklines>
            </e-chart-primaryxaxis>
            <e-chart-primaryyaxis minimum=0 maximum=70>
                <e-linestyle width="0"></e-linestyle>
                <e-majorticklines width="0"></e-majorticklines>
                <e-majorgridlines width="1" color="#eaeaea"></e-majorgridlines>
            </e-chart-primaryyaxis>
            <e-series-collection>
                <e-series dataSource="@Model.PopulationDetails" xName="Sports" yName="Boys" name="Boys" columnWidth="0.75" columnSpacing="0.5" type="@Syncfusion.EJ2.Charts.ChartSeriesType.Column">
                    <e-series-marker visible="false" width="10" height="10" shape="@Syncfusion.EJ2.Charts.ChartShape.Circle">
                        <e-series-datalabel visible="true" position="@Syncfusion.EJ2.Charts.LabelPosition.Outer" template="#Boys-Material">
                            <e-datalabelsettings-margin top="70"></e-datalabelsettings-margin>
                        </e-series-datalabel>
                    </e-series-marker>
                </e-series>
                <e-series dataSource="@Model.PopulationDetails" xName="Sports" yName="Girls" name="Girls" columnWidth="0.75" columnSpacing="0.5" type="@Syncfusion.EJ2.Charts.ChartSeriesType.Column">
                    <e-series-marker visible="false" width="10" height="10" shape="@Syncfusion.EJ2.Charts.ChartShape.Rectangle">
                        <e-series-datalabel visible="true" position="@Syncfusion.EJ2.Charts.LabelPosition.Outer" template="#Girls-Material">
                            <e-datalabelsettings-margin top="70"></e-datalabelsettings-margin>
                        </e-series-datalabel>
                    </e-series-marker>
                </e-series>
            </e-series-collection>
        </ejs-chart>
    </div>
    <div style="float: right; margin-right: 10px;">
    </div>
    <script src="~/scripts/chart/theme-color.js"></script>
    <script>
        var theme;
        function load(args) {
            loadChartTheme(args);
            args.chart.width = ej.base.Browser.isDevice ? "100%" : "75%";
            theme = args.chart.theme;
        };

        function textRender(args) {
            args.template = '#' + args.series.name + '-' + theme;
        }
    </script>
