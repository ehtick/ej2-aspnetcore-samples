@page
@using Syncfusion.EJ2
@section ControlsSection{

<div class="control-section" align="center">
    <h4 id="default" style="font-family: Segoe UI;font-weight: 500; font-style:normal; font-size:15px;">Bitcoin (USD) Price Range</h4>
    <div id="container">
        <ejs-rangenavigator id="container" valueType="DateTime" load="onChartLoad" labelFormat="MMM-yy">
            <e-rangenavigator-navigatorstylesettings unselectedRegionColor="transparent" ></e-rangenavigator-navigatorstylesettings>
            <e-rangenavigator-tooltip enable="true" displayMode="Always" format="MM/dd/yyyy"></e-rangenavigator-tooltip>
            <e-rangenavigator-rangenavigatorseriescollection>
                <e-rangenavigator-rangenavigatorseries xName="x" yName="y" type="Area" width="2">
                </e-rangenavigator-rangenavigatorseries>
            </e-rangenavigator-rangenavigatorseriescollection>
        </ejs-rangenavigator>
    </div>
</div>
    <script src="~/scripts/range-navigator/default_data.js"></script>
    <script src="~/scripts/range-navigator/theme-color.js"></script>
    <script>
        var data = bitCoinData;
        function onChartLoad(args) {
            var theme = loadRangeNavigatorTheme(args);
            args.rangeNavigator.theme = theme;
            args.rangeNavigator.width = ej.base.Browser.isDevice ? "100%" : "80%";
            args.rangeNavigator.series[0].dataSource = data;
            args.rangeNavigator.series[0].border = { width: 2, color: borderColor[themesList.indexOf(theme.toLowerCase())] };
            args.rangeNavigator.series[0].fill = 'url(#' + theme.toLowerCase() + '-gradient-chart)';
            args.rangeNavigator.value =  [new Date('2017-09-01'), new Date('2018-02-01')];
            args.rangeNavigator.navigatorStyleSettings.selectedRegionColor = regionColor[themesList.indexOf(theme.toLowerCase())];
        }
    </script>
}

