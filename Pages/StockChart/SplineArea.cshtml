@page
@using Syncfusion.EJ2
@section ControlsSection{
    @{
        var series = new String[0];
        var indicator = new string[0];
    }

    <div class="control-section" align="center">

        <ejs-stockchart id="stockchartsplinearea" load="stockload" title="Google Stock Price" seriesType="series" indicatorType="indicator">
            <e-stockchart-crosshairsettings enable="true" dashArray="5,5" snapToData="true"></e-stockchart-crosshairsettings>
            <e-stockchart-border width="0"></e-stockchart-border>
            <e-stockchart-tooltipSettings enable="true" format="<b>${point.x}</b> <br>Stock Price : <b>${point.y}</b>" header="" enableMarker="false"></e-stockchart-tooltipSettings>
            <e-stockchart-primaryxaxis>
                <e-majorgridlines width="0"></e-majorgridlines>
                <e-crosshairtooltip enable="true"></e-crosshairtooltip>
            </e-stockchart-primaryxaxis>
            <e-stockchart-primaryyaxis>
                <e-linestyle width="0" color="transparent"></e-linestyle>
                <e-majorticklines height="0" color="transparent"></e-majorticklines>
                <e-crosshairtooltip enable="true"></e-crosshairtooltip>
            </e-stockchart-primaryyaxis>
            <e-stockchart-series-collection>
                <e-stockchart-series type='SplineArea' xName='x' yName="high" name='Apple Inc' opacity=0.5>
                </e-stockchart-series>
            </e-stockchart-series-collection>
        </ejs-stockchart>
    </div>
