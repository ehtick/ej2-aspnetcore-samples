    <script src="~/scripts/StockChartData//googl.js"></script>
    <script src="~/scripts/StockChartData/theme-color.js"></script>
    <script>
        var data = googl;
        function stockload(args) {
            var selectedTheme = loadStockChartTheme(args);
            args.stockChart.series[0].dataSource = data;
            args.stockChart.series[0].fill = 'url(#' + selectedTheme.toLowerCase() + '-gradient-chart)';
            args.stockChart.series[0].border = { width: 2, color:borderColor[themes.indexOf(args.stockChart.theme.toLowerCase())] };
            args.stockChart.crosshair.lineType = 'Both';
        }
    </script>
