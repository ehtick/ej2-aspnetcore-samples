@page
@using Syncfusion.EJ2

@section ControlsSection{
<div class="control-section">
    <div id="target" class="fab-position-container custom-index">
        <ejs-fab id="fab1" target="#target" title="Top Left" position="TopLeft" cssClass="e-success" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab2" target="#target" title="Top Center" position="TopCenter" cssClass="e-warning" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab3" target="#target" title="Top Right" position="TopRight" cssClass="e-success" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab4" target="#target" title="Middle Left" position="MiddleLeft" cssClass="e-warning" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab5" target="#target" title="Middle Center" position="MiddleCenter" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab6" target="#target" title="Middle Right" position="MiddleRight" cssClass="e-warning" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab7" target="#target" title="Bottom Left" position="BottomLeft" cssClass="e-success" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab8" target="#target" title="Bottom Center" position="BottomCenter" cssClass="e-warning" iconCss="fab-icons fab-icon-people"></ejs-fab>
        <ejs-fab id="fab9" target="#target" title="Bottom Right" position="BottomRight" cssClass="e-success" iconCss="fab-icons fab-icon-people"></ejs-fab>
    </div>
</div>
}
<style>
    .fab-position-container {
        position: relative;
        min-height: 310px;
        height: 400px;
        min-width: 100%;
    }

    .custom-index {
        z-index: 500 !important;
    }

    @@font-face {
        font-family: 'fab-icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    [class^="fab-icon-"],
    [class*=" fab-icon-"] {
        font-family: 'fab-icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: inherit;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .fab-icon-people:before {
        content: "\e70a";
    }
</style>
@section Meta{
<meta name="description" content="This example demonstrates the Positions in ASP.NET Core FAB control. Explore here for more details." />
                }
@section ActionDescription{
<div id="action-description">
    <p>This sample demonstrates the different positions of the Floating Action Button in the target container. </p>
</div>
}
@section Description{
<div id="description">
    <p>Use the <code>position</code> property to change the position ofthe FAB in the target element.</p>
</div>
}
@section Title{
<title>ASP.NET Core Floating Action Button Positions Example - Syncfusion Demos </title>
                }
@section Header{
<h1 class='sb-sample-text'>Example of Positions in ASP.NET Core Floating Action Button Control</h1>
                }