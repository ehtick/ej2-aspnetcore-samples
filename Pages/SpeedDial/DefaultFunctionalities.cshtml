@page
@model EJ2CoreSampleBrowser.Pages.SpeedDial.DefaultFunctionalitiesModel
@using Syncfusion.EJ2
@using Syncfusion.EJ2.Buttons

@section ControlsSection{
<div class="control-section">
    <div id="speed-dial-wrapper">
        <div id="target" class="speeddial-default-target custom-index">
            <ejs-speeddial id="speeddial" target="#target" position="BottomCenter"  title="Menu"  items="@Model.Items" openIconCss="e-icons e-justify" closeIconCss="e-icons e-close"></ejs-speeddial>
        </div>
    </div>
</div>
}
<style>
    .speeddial-default-target {
        min-height: 300px;
        position: relative;
        margin-bottom: 35px;
    }

    .custom-index {
        z-index: 500 !important;
    }
</style>


@section Meta{
<meta name="description" content="This example demonstrates the Default Functionalities in ASP.NET Core SpeedDial control. Explore here for more details." />
    }

@section ActionDescription{
<div id="action-description">
    <p>This sample demonstrates the default functionalities of the Speed Dial component. Speed Dial is an transition type of FAB which displays a list of action buttons when clicked. </p>
</div>
}
@section Description{
<div id="description">
    <p>The Speed Dial component used to display multiple action items for the floating action button. It is useful when there are more than one primary action on the page. The Speed Dial displays action items in linear and radial directions. </p>
</div>
}

@section Title{
<title>ASP.NET Core SpeedDial Default Functionalities Example - Syncfusion Demos </title>
            }
@section Header{
<h1 class='sb-sample-text'>Example of Default Functionalities in ASP.NET Core SpeedDial Control</h1>
            }